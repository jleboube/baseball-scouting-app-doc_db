# Baseball Scouting App - Environment Variables
# Copy this to .env and update the values

# Database password (change this!)
DB_PASSWORD=change_this_secure_password

# Session secret for authentication (change this!)
SESSION_SECRET=change_this_to_a_long_random_string_minimum_32_characters

# Your domain name
DOMAIN=scouting-report.com

# Environment
NODE_ENV=production